<?xml version="1.0" encoding="utf-8"?>

                                                  
                                                        

    

                 
                     
                                                                               
           

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".views.privacypolicy.PrivacyPolicyFragment" android:tag="layout/privacy_policy_fragment_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" xmlns:app="http://schemas.android.com/apk/res-auto">

        <com.google.android.material.appbar.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.appbar.MaterialToolbar
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                 >
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/ivPrivacyPolicyBtn"
                    android:layout_width="@dimen/_30sdp"
                    android:padding="@dimen/_5sdp"
                    android:layout_height="@dimen/_25sdp"
                    android:layout_gravity="start"
                    android:tag="binding_1"          
                    android:src="@drawable/ic_back_arrow_button"
                    app:layout_constraintStart_toStartOf="parent" />
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvPrivacyPolicy"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:fontFamily="@font/inter_medium"
                    android:paddingVertical="@dimen/_1sdp"
                    android:text="Privacy Policy"
                    android:textSize="18sp"
                    android:tag="binding_2"          
                    android:layout_gravity="center_horizontal"
                    android:textColor="@color/tool_bar_black_color"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

            </com.google.android.material.appbar.MaterialToolbar>

        </com.google.android.material.appbar.AppBarLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:id="@+id/clPrivacyPolicyMain"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            android:layout_height="match_parent">


        </androidx.constraintlayout.widget.ConstraintLayout>



    </androidx.coordinatorlayout.widget.CoordinatorLayout>
         